{
  "name": "http-chain",
  "version": "0.1.0-alpha.2",
  "description": "Chained and declarative HTTP client based on the browser Fetch API",
  "main": "index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "jest",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "postcommit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muwenzi/http-chain.git"
  },
  "keywords": [
    "http",
    "http",
    "rest",
    "request",
    "chain",
    "fetch"
  ],
  "author": "Kimi Gao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muwenzi/http-chain/issues"
  },
  "homepage": "https://github.com/muwenzi/http-chain",
  "dependencies": {
    "file-saver": "^2.0.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^4.19.1",
    "husky": "^3.1.0",
    "jest": "^23.1.0",
    "lodash": "^4.17.10",
    "standard": "^14.3.1",
    "standard-version": "^7.0.1"
  }
}
